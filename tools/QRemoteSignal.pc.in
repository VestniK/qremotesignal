prefix=@CMAKE_INSTALL_PREFIX@
libdir=@ABS_LIB_DIR@
includedir=@ABS_INCLUDE_DIR@

Name: QRemoteSignal
Description: Remote Qt4 signal/slot call library
Version: @VERSION_STRING@
URL: qremotesignal.googlecode.com
Requires: QtCore >= 4.5.0, QJson
Libs: -L${libdir} -lQRemoteSignal
Libs.private: -lqjson
Cflags: -I${prefix}/include -I${includedir}
